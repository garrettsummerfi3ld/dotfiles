{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "nord0": "#2E3440",
    "nord1": "#3B4252",
    "nord2": "#434C5E",
    "nord3": "#4C566A",
    "nord4": "#D8DEE9",
    "nord5": "#E5E9F0",
    "nord6": "#ECEFF4",
    "nord7": "#8FBCBB",
    "nord8": "#88C0D0",
    "nord9": "#81A1C1",
    "nord10": "#5E81AC",
    "nord11": "#BF616A",
    "nord12": "#D08770",
    "nord13": "#EBCB8B",
    "nord14": "#A3BE8C",
    "nord15": "#B48EAD"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "p:nord11",
          "template": "# "
        },
        {
          "foreground": "p:nord9",
          "style": "plain",
          "template": "{{.UserName}}{{if .SSHSession}}@{{.HostName}}{{end}} ",
          "type": "session"
        },
        {
          "foreground": "p:nord5",
          "properties": {
            "home_icon": "~",
            "style": "folder"
          },
          "style": "powerline",
          "template": "ÔÑÅ {{ path .Path .Location }} ",
          "type": "path"
        },
        {
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:nord13{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:nord12{{ end }}",
            "{{ if gt .Ahead 0 }}p:nord15{{ end }}",
            "{{ if gt .Behind 0 }}p:nord15{{ end }}"
          ],
          "foreground": "p:nord8",
          "properties": {
            "branch_icon": "\uf418 ",
            "cherry_pick_icon": "\udb84\udc42 ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\uf419 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\uf47f ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "\uf105 {{ if .UpstreamURL }}{{url .UpstreamIcon .UpstreamURL }}{{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "p:nord11",
          "template": "{{ if eq .Code 0 }}\uf00c{{ else }}\uf071{{ end }} ",
          "properties": {
            "always_enabled": false
          }
        },
        {
          "style": "plain",
          "foreground": "p:nord6",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
